{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "frago",
  "version": "0.39.0",
  "identifier": "com.frago.client",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devUrl": "http://127.0.0.1:8093",
    "frontendDist": "../src/frago/server/assets"
  },
  "app": {
    "windows": [
      {
        "title": "frago",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "url": "http://127.0.0.1:8093"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "minimumSystemVersion": "10.15"
    },
    "windows": {
      "webviewInstallMode": {
        "type": "downloadBootstrapper"
      }
    }
  }
}
