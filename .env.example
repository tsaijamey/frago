# Google Gemini API Key
GEMINI_API_KEY=your_gemini_api_key_here

# Volcengine TTS API (火山引擎豆包语音合成)
# 从控制台获取: https://console.volcengine.com/speech/service/8
X-APP-ID=your_app_id_here
X-ACCESS-TOKEN=your_access_token_here

# Voice ID for TTS voice clone (声音复刻音色ID)
# 上传训练音频后从控制台获取
VOICE_ID=your_voice_id_here
